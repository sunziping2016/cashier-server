{
  "plugins": ["plugins/markdown"],
  "recurseDepth": 10,
  "source": {
    "include": ["dist", "package.json", "README.md"],
    "includePattern": ".+\\.js$",
    "excludePattern": ".+\\.(test|spec).js"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "template": "./node_modules/minami",
    "encoding": "utf8",
    "destination": "./doc/",
    "recurse": true
  }
}
